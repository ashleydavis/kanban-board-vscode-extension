    "glob": "^7.1.4",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "remark-parse": "^9.0.0",
