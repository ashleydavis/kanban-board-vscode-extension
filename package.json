{
  "name": "taskboard",
  "displayName": "Taskboard",
  "publisher": "ashleydavis",
  "description": "Edit your taskboard without leaving Visual Studio Code! (TODO.md format)",
  "version": "0.0.1",
  "repository": "https://github.com/ashleydavis/taskboard-vscode-extension",
  "engines": {
    "vscode": "^1.36.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "preview": true,
  "keywords": [
    "Kanban",
    "Taskboard",
    "productivity",
    "organization",
    "task management"
  ],
  "activationEvents": [
    "onCommand:extension.startExtension",
    "onWebviewPanel:taskboard-extension"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.startExtension",
        "title": "Open Taskboard"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "cd react-app && npm run build && cd .. && tsc -p ./ && copy react-app\\dist\\index.html out\\index.html",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^10.17.44",
    "@types/uuid": "^8.3.0",
    "@types/vscode": "^1.36.0",
    "dedent-js": "^1.0.1",
    "glob": "^7.1.4",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "vsce": "^1.85.0",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "remark-parse": "^9.0.0",
    "remark-stringify": "^9.0.1",
    "unified": "^9.2.0",
    "uuid": "^8.3.2"
  }
}
